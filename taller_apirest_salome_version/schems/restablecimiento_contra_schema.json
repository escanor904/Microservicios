{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "email",
        "new_password", 
        "token"
    ],
    "properties": {
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "cristiano_new@email.com"
            ]
        },
        "new_password": {
            "type": "string",
            "default": "",
            "title": "The new_password Schema",
            "examples": [
                "17"
            ]
        },
        "token": {
            "type": "string",
            "default": "",
            "title": "The new_password Schema",
            "examples": [
                "17"
            ]
        }
    },
    "examples": [{
        "email": "cristiano_new@email.com",
        "new_password": "17",
        "token": "jsahbfsipjfguweaior768t5932875t"
    }]
}